#!/bin/bash

if [ $# -ne 2 ]
    then
	echo Usage: $0 campagne_export nom_du_dossier 1>&2
        exit 1
fi

mkdir -p data/export/ds_xml > /dev/null
EXPORTDIR=data/export/ds_xml

FOLDER=$EXPORTDIR/$2

mkdir -p $FOLDER > /dev/null

DATE=$(date +%Y%m%d%H%M%S)

php symfony export:ds-xml-civa $1 $FOLDER $DATE
